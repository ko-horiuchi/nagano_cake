<%= render 'public/public_header' %>

<h1>商品一覧</h1>

    <tbody>
      <% @items.each do |item| %>
        <tr>
          <tb><%= attachment_image_tag item, :image, :fill, 300, 300, format: 'jpeg' %></tb>
          <td>
            <%= link_to item_path(item.id) do %>
            <%= item.name %>
            <% end %>
          </td>
          <td><%= item.price %></td>
        </tr>
      <% end %>
    </tbody>
